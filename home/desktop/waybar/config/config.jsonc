  "group/info-group": {
    "orientation": "inherit",
    "modules": [
      "tray",
      "network",
      "pulseaudio",
      "upower",
    ] 
  },
  "group/power-control": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 500,
      "children-class": "power-control-child",
      "transition-left-to-right": false,
    },
    "modules": [
      "custom/power", // First element is the "group leader" and won't ever be hidden
      "custom/quit",
      "custom/reboot",
    ]
  },
  "custom/quit": {
    "format": "ï‚‹ ",
    "tooltip": false,
    "on-click": "hyprctl dispatch exit"
  },
  "custom/reboot": {
