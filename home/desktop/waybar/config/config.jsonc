    "icon-size": 15,
    "hide-if-empty": false,
    "tooltip": true,
    "tooltip-spacing": 20,
    "format": " {percentage}"
  },
  "mpris": {
    "format": "{status_icon} {dynamic}",
    "format-paused": "{status_icon} {dynamic}",
    "player-icons": {
      "default": "â–¶",
      "mpv": "ğŸµ"
    },
    "status-icons": {
      "paused": "ï‹",
      "playing": "ïŒ",
    },
    "dynamic-order": ["title", "artist", "position", "length"],
    "dynamic-len": 200,
    "ignored-players": ["firefox"]
  },
  "pulseaudio": {
    // "scroll-step": 1, // %, can be a float
    "format": "{icon} {volume}%",
    "format-bluetooth": "{icon} {volume}%",
    "format-bluetooth-muted": "{icon} {volume}%",
    "format-muted": "î»¨  {volume}%",
    "format-icons": {
